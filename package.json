{
  "name": "@polymer/shop",
  "version": "3.0.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/shop",
  "scripts": {
    "start": "polymer serve",
    "start:webpack": "webpack-dev-server --open --mode development",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.js",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "mocha test/ --timeout=10000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-icon": "^3.0.0",
    "@polymer/iron-iconset-svg": "^3.0.0",
    "@polymer/iron-overlay-behavior": "^3.0.0",
    "@polymer/lit-element": "^0.6.4",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-spinner": "^3.0.0",
    "@polymer/polymer": "3.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "lit-html": "^0.14.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "polymer-cli": "^1.9.2",
    "polyserve": "^0.27.11",
    "prpl-server": "^1.3.0",
    "puppeteer": "^1.0.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
