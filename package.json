{
  "name": "@polymer/shop",
  "version": "3.0.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/shop",
  "scripts": {
    "start": "polymer serve",
    "rollup": "rimraf rollup-build && rollup -c",
    "serve": "cd rollup-build && es-dev-server --app-index=index.html",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "mocha test/ --timeout=10000"
  },
  "dependencies": {
    "@babel/core": "^7.10.1",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-icon": "^3.0.0",
    "@polymer/iron-iconset-svg": "^3.0.0",
    "@polymer/iron-overlay-behavior": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-spinner": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "babel": "^6.23.0",
    "core-js": "^3.6.5",
    "lit-element": "^2.1.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "systemjs": "^6.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "es-dev-server": "^1.54.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "polymer-cli": "^1.9.11",
    "prpl-server": "^1.3.0",
    "puppeteer": "^1.0.0",
    "rollup": "^2.11.2"
  }
}
