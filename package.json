{
  "name": "@polymer/shop",
  "flat": true,
  "version": "3.0.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "scripts": {
    "start": "polyserve --npm --module-resolution=node",
    "test": "mocha test/ --timeout=10000",
    "bundle": "polymer-bundler --module-resolution node --out-dir bundled --shell src/components/shop-app.js src/components/shop-app.js",
    "minify": "babel bundled --out-dir dist",
    "copy": "cp -r data images index.html manifest.json dist",
    "sw": "workbox generateSW workbox-config.js",
    "copy-polyfills": "cp -r node_modules/@webcomponents/webcomponentsjs .",
    "clean": "rm -rf bundled dist webcomponentsjs",
    "build": "npm run clean && npm run bundle && npm run minify && npm run copy && npm run sw && npm run copy-polyfills"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.12",
    "@polymer/iron-icon": "^3.0.0-pre.12",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.12",
    "@polymer/iron-overlay-behavior": "^3.0.0-pre.12",
    "@polymer/lit-element": "^0.2.2",
    "@polymer/paper-icon-button": "^3.0.0-pre.12",
    "@polymer/paper-spinner": "^3.0.0-pre.12",
    "@polymer/polymer": "^3.0.0-pre.12",
    "lit-html": "^0.9.0",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "pixelmatch": "^4.0.2",
    "polyserve": "^0.26.0",
    "puppeteer": "^1.0.0",
    "workbox-cli": "^3.0.0"
  }
}
